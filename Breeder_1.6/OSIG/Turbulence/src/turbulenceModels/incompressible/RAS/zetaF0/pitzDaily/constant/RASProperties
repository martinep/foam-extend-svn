{
    Cmu        0.22;
    sigmaK     1.0;
    sigmaEps   1.3;
    sigmaZeta  1.2;
    Ceps10     1.4;
    Ceps11     0.012;
    Ceps2      1.9;
    Cf1        1.4;
    Cf2        0.65;
    Ceta       85.0;
    CL         0.36;
    Ctau       6.0;
    CT         0.6;
}

